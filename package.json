    "forever": "^4.0.3",
    "fs-extra": "7.0.0",
    "mysql": "2.16.0",
    "node-gyp": "^7.1.2",
    "openport": "0.0.5",
    "prompt": "1.0.0",
    "winston": "3.1.0",
