{
  "name": "ee_backend",
  "version": "0.0.1",
  "description": "Backend technical challenge for Electricity Exchange",
  "main": "electricity.js",
  "scripts": {
    "test": "jest",
    "cli": "node ./cli/cli.js",
    "production": "./node_modules/.bin/forever -p logs/forever --minUptime 1000 --spinSleepTime 10000 --killSignal SIGINT start electricity.js production",
